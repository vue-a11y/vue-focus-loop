{
  "name": "@vue-a11y/focus-loop",
  "version": "1.0.3",
  "description": "Vue 3 component that helps you to to trap focus in an element.",
  "main": "dist/focus-loop.cjs.js",
  "browser": "dist/focus-loop.es.js",
  "module": "dist/focus-loop.es.js",
  "unpkg": "dist/focus-loop.iife.js",
  "jsdelivr": "dist/focus-loop.iife.js",
  "types": "dist/global.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/focus-loop.es.js",
      "require": "./dist/focus-loop.cjs.js"
    }
  },
  "scripts": {
    "dev": "npm run demo:dev",
    "build": "vuedx-typecheck . && vite build && npm run fixFiles",
    "fixFiles": "cp -a dist/@vue-a11y/. dist && rm -rf dist/@vue-a11y dist/favicon.ico",
    "demo:dev": "cd demo && npm run dev",
    "demo:build": "cd demo && npm run build",
    "demo:publish": "surge ./demo/dist https://vue-focus-loop.surge.sh/",
    "release": "standard-version",
    "project:publish": "git push --tags origin next && npm run build && npm publish --tag next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-a11y/vue-focus-loop.git"
  },
  "keywords": [
    "vue",
    "component",
    "a11y",
    "accessibility",
    "focus",
    "tab",
    "tab-loop",
    "focus trap",
    "focus loop",
    "vue-a11y",
    "vue.js"
  ],
  "author": "Alan Ktquez <ktquez@gmail.com> (https://github.com/ktquez)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-a11y/vue-focus-loop/issues"
  },
  "homepage": "https://github.com/vue-a11y/vue-focus-loop#readme",
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.12"
  }
}
